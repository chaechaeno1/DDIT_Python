	location.href= "emp_mod?e_id=<%=vo.e_id%>&e_name=<%=vo.e_name%>&gen=<%=vo.gen%>&addr=<%=vo.addr%>";
}

function fn_del(){
	console.log('fn_del')
	location.href= "emp_del_act?e_id=<%=vo.e_id%>";
}
</script>
